<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_enabled="true">
        <shape android:shape="rectangle">
            <solid android:color="@color/submit_btn_color" />
            <corners android:radius="@dimen/dp_15" />
            <stroke android:width="0.5dp" android:color="@color/submit_btn_color" />
        </shape>
    </item>
    <item android:state_enabled="false">
        <shape android:shape="rectangle">
            <solid android:color="@color/submit_btn_color_25" />
            <corners android:radius="@dimen/dp_15" />
            <stroke android:width="0.5dp" android:color="@color/submit_btn_color_25" />
        </shape>
    </item>
</selector>
