<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="RecyclerView"><attr format="string" name="layoutManager"/><attr name="android:orientation"/><attr format="integer" name="spanCount"/><attr format="boolean" name="reverseLayout"/><attr format="boolean" name="stackFromEnd"/></declare-styleable>
    <declare-styleable name="SwipeLayout"><attr name="drag_edge">
            <flag name="left" value="1"/>
            <flag name="right" value="2"/>
            <flag name="top" value="4"/>
            <flag name="bottom" value="8"/>
        </attr><attr format="dimension" name="leftEdgeSwipeOffset"/><attr format="dimension" name="rightEdgeSwipeOffset"/><attr format="dimension" name="topEdgeSwipeOffset"/><attr format="dimension" name="bottomEdgeSwipeOffset"/><attr format="enum" name="show_mode">
            <enum name="lay_down" value="0"/>
            <enum name="pull_out" value="1"/>
        </attr><attr format="boolean" name="clickToClose"/></declare-styleable>
    <dimen name="item_touch_helper_max_drag_scroll_per_frame">20dp</dimen>
    <dimen name="item_touch_helper_swipe_escape_max_velocity">800dp</dimen>
    <dimen name="item_touch_helper_swipe_escape_velocity">120dp</dimen>
    <item name="item_touch_helper_previous_elevation" type="id"/>
</resources>